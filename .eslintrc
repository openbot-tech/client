{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "semi": [2, "never"],
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off",
    "max-len": ["error", { "code": 120 }],
    "object-curly-newline": "off",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
